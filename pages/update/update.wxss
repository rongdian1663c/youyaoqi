
.tabbar {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 30px;
    box-shadow: 0 2px 10px #ccc;
    box-sizing: border-box;
    padding: 80 200rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
    background-color: #fff;
    border-top: 1rpx solid #f3f3f3;
    z-index: 999;
}

