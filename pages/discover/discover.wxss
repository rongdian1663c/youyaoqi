.swiper {
  height: 400rpx;
  width: 100%;
}

.swiper-image {
  height: 100%;
  width: 100%;
}

.slide-image {
  height: 100%;
  width: 100%;
}

.item image {
  width: 150rpx;
  height: 120rpx;
  display: block;
  float: left;
}

.text {
  text-align: center;
  width: 100%;
}

.menu-image {
  width: 150rpx;
  height: 120rpx;
  display: block;
  float: left;
}

.text-title {
  width: 100%;
  height: 60rpx;
  font-size: 25rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.text-name{
   width: 100%;
  height: 60rpx;
  font-size: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.cell {
  display: flex;
  flex-direction: column;
  width: calc(48%);
  float: left;
  height: 300rpx;
  margin:7.5rpx;
}

.cell-Other {
   display: flex;
  flex-direction: column;
  width: calc(30%);
  float: left;
  height: 300rpx;
  margin:7.5rpx;
}

.cell-image{
  width: 100%;
  border-radius: 10rpx;
}


