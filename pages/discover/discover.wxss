.swiper {
  height: 400rpx;
  width: 100%;
}

.swiper-image {
  height: 100%;
  width: 100%;
}

.slide-image {
  height: 100%;
  width: 100%;
}

.item image {
  width: 150rpx;
  height: 120rpx;
  display: block;
  float: left;
}

.text {
  text-align: center;
  width: 100%;

}

.menu-image {
  width: 150rpx;
  height: 120rpx;
  display: block;
  float: left;
}

.text-title {
  width: 100%;
  height: 60rpx;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: bold;
}

.text-name{
  width: 100%;
  height: 100rpx;
  font-size: 25rpx;
  display: flex;
  align-items: center;
  justify-content: left;
  font-weight: bold;
  float: left;
}

.text-intro {
  width: 100%;
  height: 100rpx;
  font-size: 20rpx;
  float: left;
}

.cell {
  display: flex;
  flex-direction: column;
  width: calc(48%);
  float: left;
  height: 300rpx;
  margin:7.5rpx;
}

.cell-Other {
   display: flex;
  flex-direction: column;
  width: calc(30%);
  float: left;
  height: 400rpx;
  margin:7.5rpx;
}

.cell-image {
  width: 100%;
  border-radius: 10rpx;
}

.button-a {
  width: 48%;
  height: 50rpx;
  float: left;
  font-size: 20rpx;
  margin-left: 10rpx;
  margin-bottom: 30rpx;

}

.button-b {
  width: 48%;
  height: 50rpx;
  font-size: 20rpx;
  margin: 0rpx,10rpx,30rpx,0rpx;
  
}

.button-c {
  width: 80%;
  height: 50rpx;
  font-size: 20rpx;
  margin-bottom: 20rpx;
  margin-top: 10rpx;
}

